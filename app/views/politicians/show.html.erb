<h4><%= @politician.title %></h4>

<div class="information">
  <p><%= @politician.first_name %> <%= @politician.last_name %></p>
  <p><%= @politician.party %></p>
  <p><%= @politician.state %></p>
  <div class="votes-with-party">
    <p><%= "#{@politician.votes_with_party}%" %></p>
  </div>
</div>

<ul class="profile">
    <% @politician.profile.each do |party, affiliation| %>
      <li><%= party %>: <%= affiliation %></li>
    <% end %>
</ul>
